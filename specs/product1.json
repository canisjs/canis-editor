{
    "charts": [
        {
            "source": "./charts/product1.dsvg"
        },
        {
            "source": "./charts/product2.dsvg"
        }
    ],
    "animations": [
        {
            "id": "dot",
            "selector": ".symbol",
            "elements": {
                "enter": {
                    "effects": [
                        {
                            "type": "fade",
                            "duration": 200
                        }
                    ]
                },
                "update": {
                    "align": {
                        "target": "dot.enter.effects[0]",
                        "type": "object"
                    },
                    "reference": "start after previous",
                    "effects": [
                        {
                            "type": "translate X",
                            "duration": 600
                        },
                        {
                            "reference": "start after previous",
                            "type": "translate Y",
                            "duration": 600
                        },
                        {
                            "reference": "start after previous",
                            "type": "scale X",
                            "duration": 600
                        },
                        {
                            "type": "scale Y",
                            "duration": 600
                        }
                    ]
                },
                "exit": {
                    "align": {
                        "target": "dot.update.effects[0]",
                        "type": "object"
                    },
                    "reference": "start after previous",
                    "effects": [
                        {
                            "type": "fade out",
                            "duration": 600
                        }
                    ]
                }
            }
        }
    ]
}